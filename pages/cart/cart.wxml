<!-- pages/cart/cart.wxml -->
<view
 wx:if="{{!isEmpty}}"
 class="container"
>
	<block
	 wx:for="{{cartItems}}"
	 wx:key="index"
	>
		<u-cart-item
		 bind:overflow="onOverflow"
		 bind:itemdelete="onDeleteItem"
		 bind:countfloat="onCountFloat"
		 bind:itemcheck="onSingleCheck"
		 cart-item="{{item}}"
		/>
	</block>
</view>

